{
  "name": "next-boiler-plate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:dev": "NODE_ENV=development DEV=1 next build",
    "build:staging": "NODE_ENV=development STAGING=1 next build",
    "build:prod": "PRODUCTION=1 next build",
    "deploy": "zip -r ./bundle.zip ./.next ./public ./package.json ./yarn.lock",
    "start": "next start",
    "test:watch": "jest --watchAll --verbose --coverage",
    "lint": "eslint 'src/**/*'",
    "lint:tsc": "tsc --noEmit",
    "lint:fix": "eslint 'src/**/*' --fix"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime-corejs3": "^7.12.5",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "axios": "^0.19.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.8.1",
    "css-in-js-media": "^1.0.15",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^2.5.5",
    "https": "^1.0.0",
    "immer": "^7.0.5",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "next": "^10.2.3",
    "next-redux-wrapper": "^7.0.2",
    "normalize.css": "^8.0.1",
    "query-string": "^6.13.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-responsive": "^8.2.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7",
    "utility-types": "^3.10.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.9",
    "@types/jest-plugin-context": "^2.9.4",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-redux": "^7.1.16",
    "@types/react-responsive": "^8.0.2",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/yup": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^6.0.0",
    "jest": "^26.4.0",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.2.0",
    "typescript": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:tsc && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,scss,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
